{% render 'breadcrumbs' %}
<product-info
  id="MainProduct-{{ section.id }}"
  class="section-{{ section.id }}-padding gradient color-{{ section.settings.color_scheme }}"
  data-section="{{ section.id }}"
  data-product-id="{{ product.id }}"
  data-update-url="true"
  data-url="{{ product.url }}"
  {% if section.settings.image_zoom == 'hover' %}
    data-zoom-on-hover
  {% endif %}
>
 <div class="container pt-5 pb-20">
    <div class="grid grid-cols-4 md:grid-cols-12 col-start-1">
      <div class="grid grid-cols-1 col-span-4 gap-2 md:col-span-6 md:gap-12 h-fit">

         {% comment %} Mobile product info {% endcomment %}
          <div x-data class="flex flex-col mb-2 md:hidden">
          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when '@app' -%}
                {% render block %}
              {%- when 'text' -%}
                <p
                  class="product__text inline-richtext{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}"
                  {{ block.shopify_attributes }}
                >
                  {{- block.settings.text -}}
                </p>
              {%- when 'title' -%}
                <div class="flex flex-col gap-2 mt-5 mb-2">
                <div class="product__title mb-2" {{ block.shopify_attributes }}>
                  <h1 class="text-5xl">{{ product.title | escape }}</h1>
                </div>
                </div>
              {%- when 'vendor'-%}
                <div class="flex items-center justify-between">
                  <span class="py-1.5 px-2.5 border border-gray-200 rounded-sm w-fit uppercase">{{ product.vendor }}</span>
                  <p
                    class="text-sm text-gray-700 font-normal"
                    id="Sku-{{ section.id }}"
                    role="status"
                    {{ block.shopify_attributes }}
                  >
                    <span class="font-semibold">{{ 'products.product.sku' | t }}:</span>
                    {{- product.selected_or_first_available_variant.sku -}}
                  </p>
                </div>
              {%- when 'price' -%}
                <div id="price-{{ section.id }}" class="flex flex-col gap-2" role="status" {{ block.shopify_attributes }}>
                  {%- render 'price',
                    product: product,
                    use_variant: true,
                    show_badges: true,
                    price_class: 'price--large'
                  -%}
                <div {{ block.shopify_attributes }}>
                  {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                  {% comment %} Klarna goes here {% endcomment %}
                  <div class="w-full bg-gray-100 p-2.5 text-sm font-normal flex items-center justify-between">
                    <div class="flex flex-col">
                    <span>4 interest-free payments of $12.74.</span>
                    <a href="#" class="text-brand underline">Learn more</a>
                    </div>
                    {% render 'icon', icon: 'klarna-logo', class: '!w-10' %}
                  </div>
                </div>
              </div>
              {%- endcase -%}
            {%- endfor -%}
              <div class="mt-2">
                <img class="h-5" src="{{ section.settings.trust_pilot | img_url: '400x' }}">
              </div>
            </div>
          {% comment %} Mobile product info end {% endcomment %}

        {% render 'product-media-gallery', variant_images: variant_images %}

         <div class="hidden md:block">
         {%- for block in section.blocks -%}
          {%- case block.type -%}
          {%- when 'description' -%}
            {%- if product.description != blank -%}
              {% render 'product-description', tab_left: block.settings.tab_heading_left, tab_right: block.settings.tab_heading_right %}
              {%- endif -%}
          {%- endcase -%}
        {%- endfor -%}
        </div>
      
      </div>
     
      <div class="order-1 col-span-4 md:col-span-5 md:col-start-8">
        <section
          x-data
          id="ProductInfo-{{ section.id }}"
          class="flex flex-col gap-6"
        >
          {%- assign product_form_id = 'product-form-' | append: section.id -%}
          
          
             {% comment %} Desktop product info {% endcomment %}
              <div class="hidden md:flex flex-col gap-6">
              {%- for block in section.blocks -%}
                {%- case block.type -%}
                  {%- when '@app' -%}
                    {% render block %}
                  {%- when 'text' -%}
                    <p
                      class="product__text inline-richtext{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}"
                      {{ block.shopify_attributes }}
                    >
                      {{- block.settings.text -}}
                    </p>
                  {%- when 'title' -%}
                    <div class="flex flex-col gap-2">
                    <p
                      class="text-sm text-gray-700 font-normal"
                      id="Sku-{{ section.id }}"
                      role="status"
                      {{ block.shopify_attributes }}
                    >
                      <span class="font-semibold">{{ 'products.product.sku' | t }}:</span>
                      {{- product.selected_or_first_available_variant.sku -}}
                    </p>
                    <div class="product__title" {{ block.shopify_attributes }}>
                      <h1 class="text-5xl">{{ product.title | escape }}</h1>
                    </div>
                    </div>
                  {%- when 'vendor'-%}
                    <div class="flex justify-between items-center">
                    <span class="py-1.5 px-2.5 border border-gray-200 rounded-sm w-fit uppercase">{{ product.vendor }}</span>
                    <img src="{{ section.settings.trust_pilot | img_url: '400x' }}" alt="Trust Pilot" class="h-5">
                    </div>
                  {%- when 'price' -%}
                    <div id="price-{{ section.id }}" class="flex flex-col gap-2" role="status" {{ block.shopify_attributes }}>
                      {%- render 'price',
                        product: product,
                        use_variant: true,
                        show_badges: true,
                        price_class: 'price--large'
                      -%}
                    <div {{ block.shopify_attributes }}>
                      {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                      {% comment %} Klarna goes here {% endcomment %}
                      <div class="w-full bg-gray-100 p-2.5 text-sm font-normal flex items-center justify-between">
                        <div>
                        <span>4 interest-free payments of $12.74.</span>
                        <a href="#" class="text-brand underline">Learn more</a>
                        </div>
                        {% render 'icon', icon: 'klarna-logo', class: '!w-10' %}
                      </div>
                    </div>
                  </div>
                  {%- endcase -%}
                {%- endfor -%}
                </div>

          {% comment %} Desktop product info end {% endcomment %}
          
            {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when 'custom_liquid' -%}
                {{ block.settings.custom_liquid }}

                {% liquid
                  assign share_url = product.selected_variant.url | default: product.url | prepend: request.origin
                  render 'share-button', block: block, share_link: share_url
                %}

              {%- when 'product_form' -%}
              {% render 'product-form', product: product, block: block, , product_form_id: product_form_id %}
              
               
              {%- when 'rating' -%}
                {%- if product.metafields.reviews.rating.value != blank -%}
                  <div class="rating-wrapper">
                    {% liquid
                      assign rating_decimal = 0
                      assign decimal = product.metafields.reviews.rating.value.rating | modulo: 1
                      if decimal >= 0.3 and decimal <= 0.7
                        assign rating_decimal = 0.5
                      elsif decimal > 0.7
                        assign rating_decimal = 1
                      endif
                    %}
                    <div
                      class="rating"
                      role="img"
                      aria-label="{{ 'accessibility.star_reviews_info' | t: rating_value: product.metafields.reviews.rating.value, rating_max: product.metafields.reviews.rating.value.scale_max }}"
                    >
                      <span
                        aria-hidden="true"
                        class="rating-star"
                        style="--rating: {{ product.metafields.reviews.rating.value.rating | floor }}; --rating-max: {{ product.metafields.reviews.rating.value.scale_max }}; --rating-decimal: {{ rating_decimal }};"
                      ></span>
                    </div>
                    <p class="rating-text caption">
                      <span aria-hidden="true">
                        {{- product.metafields.reviews.rating.value }} /
                        {{ product.metafields.reviews.rating.value.scale_max -}}
                      </span>
                    </p>
                    <p class="rating-count caption">
                      <span aria-hidden="true">({{ product.metafields.reviews.rating_count }})</span>
                      <span class="visually-hidden">
                        {{- product.metafields.reviews.rating_count }}
                        {{ 'accessibility.total_reviews' | t -}}
                      </span>
                    </p>
                  </div>
                {%- endif -%}
                {%- when 'store_availability' %}
                {% render 'store-availability', product: product, block: block %}
          
            {%- endcase -%}
          {%- endfor -%}

        </section>
         <div class="block md:hidden">
            {%- for block in section.blocks -%}
              {%- case block.type -%}
              {%- when 'description' -%}
                {%- if product.description != blank -%}
                  {% render 'product-description', block: block, tab_left: block.settings.tab_heading_left, tab_right: block.settings.tab_heading_right  %}
                  {%- endif -%}
              {%- endcase -%}
            {%- endfor -%}
            </div>
        </div>
      </div>


    {% comment %} {% render 'product-media-modal', variant_images: variant_images %} {% endcomment %}

    {% assign popups = section.blocks | where: 'type', 'popup' %}
    {%- for block in popups -%}
      <modal-dialog id="PopupModal-{{ block.id }}" class="product-popup-modal" {{ block.shopify_attributes }}>
        <div
          role="dialog"
          aria-label="{{ block.settings.text }}"
          aria-modal="true"
          class="product-popup-modal__content"
          tabindex="-1"
        >
          <button
            id="ModalClose-{{ block.id }}"
            type="button"
            class="product-popup-modal__toggle"
            aria-label="{{ 'accessibility.close' | t }}"
          >
            {% render 'icon-close' %}
          </button>
          <div class="product-popup-modal__content-info">
            <h1 class="h2">{{ block.settings.page.title | escape }}</h1>
            {{ block.settings.page.content }}
          </div>
        </div>
      </modal-dialog>
    {%- endfor -%}

    {%- if product.media.size > 0 -%}
      <script src="{{ 'product-modal.js' | asset_url }}" defer="defer"></script>
      <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    {%- if first_3d_model -%}
      <script type="application/json" id="ProductJSON-{{ product.id }}">
        {{ product.media | where: 'media_type', 'model' | json }}
      </script>
      <script src="{{ 'product-model.js' | asset_url }}" defer></script>
    {%- endif -%}

    {%- liquid
      if product.selected_or_first_available_variant.featured_media
        assign seo_media = product.selected_or_first_available_variant.featured_media
      else
        assign seo_media = product.featured_media
      endif
    -%}

    <script type="application/ld+json">
      {{ product | structured_data }}
    </script>
  </div>
</product-info>

